// The format of this file is specified in https://probe.rs/docs/tools/debugger/#start-a-debug-session-with-minimum-configuration
{
  "version": "0.2.0",
  "configurations": [
    {
      "preLaunchTask": "rust: cargo build",
      "type": "probe-rs-debug",
      "request": "launch",
      "name": "pico-util",
      "cwd": "${workspaceFolder}",
      "chip": "rp2350",
      // RP2040 doesn't support connectUnderReset
      "connectUnderReset": false,
      "speed": 4000,
      "runtimeExecutable": "probe-rs",
      "runtimeArgs": [
        "dap-server"
      ],
      "flashingConfig": {
        "flashingEnabled": true,
        "haltAfterReset": false
      },
      "coreConfigs": [
        {
          "coreIndex": 0,
          "programBinary": "target/thumbv8m.main-none-eabihf/debug/pico-util",
          "rttEnabled": true,
          "svdFile": "${home}/Developer/pico-sdk/src/rp2350/hardware_regs/RP2350.svd",
        }
      ],
      "consoleLogLevel": "Debug", //Error, Warn, Info, Debug, Trace
      "wireProtocol": "Swd"
    }
  ]
}